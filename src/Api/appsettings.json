{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "None"
    }
  },
  "Expires": {
    "AccessToken": "00:05",
    "RefreshToken": "90"
  },
  "Avatar": {
    "MaxSize": 1048576,
    "MaxResolution": {
      "Width": 350,
      "Height": 350
    },
    "PossibleExtensions": [
      ".jpg",
      ".jpeg",
      ".png"
    ]
  },
  "CoverImage": {
    "MaxSize": 1048576,
    "MaxResolution": {
      "Width": 1280,
      "Height": 720
    },
    "PossibleExtensions": [
      ".jpg",
      ".jpeg",
      ".png"
    ]
  },
  "AllowedHosts": "*"
}
