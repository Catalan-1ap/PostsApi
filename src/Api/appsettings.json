{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "None"
    }
  },
  "Expires": {
    "AccessToken": "00:05",
    "RefreshToken": "90"
  },
  "Avatar": {
    "MaxSize": 1048576,
    "MaxWidth": 350,
    "MaxHeight": 350,
    "PossibleExtensions": [
      ".jpg",
      ".jpeg",
      ".png"
    ]
  },
  "AllowedHosts": "*"
}
